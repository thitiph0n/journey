{
  "name": "journey",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "dev:web": "cd web && bun run dev",
    "dev:all": "echo 'Run dev and dev:web in separate terminals'",
    "build": "bun run build:web",
    "build:web": "cd web && bun run build",
    "deploy": "bun run build && wrangler deploy",
    "preview": "wrangler dev",
    "type-check": "cd web && bun run type-check",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.6.3",
    "wrangler": "^4.46.0"
  },
  "dependencies": {
    "elysia": "^1.4.13",
    "@elysiajs/eden": "^1.2.5",
    "@elysiajs/jwt": "^1.2.0",
    "@elysiajs/cors": "^1.2.1"
  }
}
